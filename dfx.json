{
  "canisters": {
    "publish": {
      "build": "cargo build --target wasm32-unknown-unknown --package  publish --release",
      "candid": "src/publish/src/publish.did",
      "wasm": "target/wasm32-unknown-unknown/release/publish.wasm",
      "type": "custom"
    },
    "subscriberA": {
      "build": "cargo build --target wasm32-unknown-unknown --package  subscriber --release",
      "candid": "src/subscriber/src/subscriber.did",
      "wasm": "target/wasm32-unknown-unknown/release/subscriber.wasm",
      "type": "custom",
      "dependencies": [
        "publish"
      ]
    },
    "subscriberB": {
      "build": "cargo build --target wasm32-unknown-unknown --package  subscriber --release",
      "candid": "src/subscriber/src/subscriber.did",
      "wasm": "target/wasm32-unknown-unknown/release/subscriber.wasm",
      "type": "custom",
      "dependencies": [
        "publish"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}